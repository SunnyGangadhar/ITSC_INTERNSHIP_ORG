{
  "name": "react-static-example",
  "private": true,
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "bundle": "react-static bundle",
    "export": "react-static export",
    "serve": "serve dist -p 3000 -s",
    "predeploy": "npm run build",
    "deploy": "netlify deploy --dir dist --message \"$(git show -s --format='%h %s')\"",
    "deploy:prod": "npm run deploy -- --prod",
    "lint": "eslint src static.config.js --ext .js,.jsx",
    "lint:fix": "eslint src static.config.js --ext .js,.jsx --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@reach/router": "^1.2.1",
    "apexcharts": "^3.25.0",
    "clsx": "^1.1.1",
    "framer-motion": "^2.9.5",
    "google-map-react": "^2.1.9",
    "hamburgers": "^1.1.3",
    "leaflet": "^1.7.1",
    "netlify-cli": "^2.71.0",
    "pe7-icon": "^1.0.4",
    "react": "^16.14.0",
    "react-apexcharts": "^1.3.7",
    "react-dom": "^16.14.0",
    "react-flag-kit": "^0.3.1",
    "react-ionicons": "^3.1.4",
    "react-leaflet": "^2.8.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-static": "^7.5.3",
    "react-static-plugin-reach-router": "^7.5.3",
    "react-static-plugin-sass": "^7.3.0",
    "react-static-plugin-sitemap": "^7.5.3",
    "react-static-plugin-source-filesystem": "^7.5.3",
    "reactstrap": "^8.9.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-react-tools": "^1.1.7",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "react-hot-loader": "^4.13.0",
    "serve": "^11.1.0",
    "stylelint": "^13.11.0",
    "stylelint-config-recommended": "^3.0.0"
  }
}
